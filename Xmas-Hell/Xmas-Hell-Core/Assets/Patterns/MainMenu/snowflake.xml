<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE bulletml SYSTEM "bulletml.dtd">
<bulletml>
  <action label="top">
    <fire>
      <bullet>
        <direction type="aim">0</direction>
        <speed>4</speed>
        <action>
          <wait>60</wait>
          <vanish />
          <repeat>
            <times>5</times>
            <action>
              <fire>
                <bullet>
                  <direction type="sequence">360 / 5</direction>
                  <speed>10</speed>
                </bullet>
              </fire>
            </action>
          </repeat>
        </action>
      </bullet>
    </fire>
  </action>
</bulletml>